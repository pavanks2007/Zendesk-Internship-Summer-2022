
# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.Default.redirect(to = "/getAllTickets")
GET     /getAllTickets              controllers.TicketRequestController.getAllTickets
POST    /getAllTickets              controllers.TicketRequestController.postAllTickets
GET     /getTicketInfoById          controllers.TicketRequestController.getTicketInfoById
POST    /getTicketInfoById          controllers.TicketRequestController.postTicketInfoById

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

      